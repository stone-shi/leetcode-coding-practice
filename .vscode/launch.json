    "version": "0.2.0",
    "configurations": [


        {
            "type": "java",
            "name": "Launch with Arguments Prompt",
            "request": "launch",
            "mainClass": "com.shifamily.dev.App",
            "args": "${command:SpecifyProgramArgs}"
        },

        {
            "type": "java",
            "name": "Launch with current class",
            "request": "launch",
            "mainClass": "com.shifamily.dev.App",
            "args": "${fileBasenameNoExtension}"
          
        }

    ]
}